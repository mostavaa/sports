@{
    ViewBag.Title = "Home Page";


}

<div id="app">
    <div id="latest5">
        <div class="container-fluid paddding mb-5">
            <div class="row mx-0">
                <div class="col-md-6 col-12 paddding animate-box" data-animate-effect="fadeIn">
                    <div class="fh5co_suceefh5co_height">
                        <img v-bind:src=" imagesUrl + firstItem.imageName" alt="img" />
                        <div class="fh5co_suceefh5co_height_position_absolute"></div>
                        <div class="fh5co_suceefh5co_height_position_absolute_font">
                            <div class="">
                                <a href="#" class="color_fff">
                                    <i class="fa fa-clock-o"></i>&nbsp;&nbsp;{{firstItem.date}}
                                </a>
                            </div>
                            <div class="">
                                <a v-bind:href="firstItem.link" class="fh5co_good_font"> {{firstItem.desc}} </a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="row">
                        <div class="col-md-6 col-6 paddding animate-box" data-animate-effect="fadeIn" v-for="item in latestItems">
                            <div class="fh5co_suceefh5co_height_2">
                                <img v-bind:src="imagesUrl + item.imageName" alt="img" />
                                <div class="fh5co_suceefh5co_height_position_absolute"></div>
                                <div class="fh5co_suceefh5co_height_position_absolute_font_2">
                                    <div class="">
                                        <a href="#" class="color_fff">
                                            <i class="fa fa-clock-o"></i>&nbsp;&nbsp;{{item.date}}
                                        </a>
                                    </div>
                                    <div class="">
                                        <a v-bind:href="item.desc" class="fh5co_good_font_2"> {{item.desc}} </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="latest-5-s-1">

        <div class="container-fluid pt-3">
            <div class="container animate-box" data-animate-effect="fadeIn">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">{{latest5s1Header}}</div>
                </div>
                <div class="owl-carousel owl-theme js" id="slider1">
                    <div class="item px-2" v-for="item in latest5s1Items">
                        <div class="fh5co_latest_trading_img_position_relative">
                            <div class="fh5co_latest_trading_img">
                                <img v-bind:src="imagesUrl+item.imageName" alt=""
                                     class="fh5co_img_special_relative" />
                            </div>
                            <div class="fh5co_latest_trading_img_position_absolute"></div>
                            <div class="fh5co_latest_trading_img_position_absolute_1">
                                <a v-bind:href="item.link" class="text-white"> {{item.desc}} </a>
                                <div class="fh5co_latest_trading_date_and_name_color"> {{item.date}} </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <div id="latest-5-s2">

        <div class="container-fluid pb-4 pt-5">
            <div class="container animate-box">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">{{latest4s2Header}}</div>
                </div>
                <div class="owl-carousel owl-theme" id="slider2">
                    <div class="item px-2" v-for="item in latest4s2Items">
                        <div class="fh5co_hover_news_img">
                            <div class="fh5co_news_img">
                                <img v-bind:src="imagesUrl+item.imageName" alt="" />
                            </div>
                            <div>
                                <a v-bind:href="item.link" class="d-block fh5co_small_post_heading"><span class="">{{item.desc}}</span></a>
                                <div class="c_g">
                                    <i class="fa fa-clock-o"></i> {{item.date}}
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <div id="latest-5-s3">

        <div class="container-fluid pb-4 pt-5">
            <div class="container animate-box">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">{{latest4s3Header}}</div>
                </div>
                <div class="owl-carousel owl-theme" id="slider3">
                    <div class="item px-2" v-for="item in latest4s3Items">
