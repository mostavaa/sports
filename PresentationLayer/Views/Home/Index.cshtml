                            <div class="fh5co_news_img">
                                <img v-bind:src="imagesUrl+item.imageName" alt="" />
                            </div>
                            <div>
                                <a v-bind:href="item.link" class="d-block fh5co_small_post_heading"><span class="">{{item.desc}}</span></a>
                                <div class="c_g">
                                    <i class="fa fa-clock-o"></i> {{item.date}}
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <div>


        <div class="container-fluid pb-4 pt-4 paddding">
            <div class="container paddding">
                <div class="row mx-0">
                    <div class="col-md-8 animate-box" data-animate-effect="fadeInLeft">
                        <div>
                            <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">{{listHeader}}</div>
                        </div>
                        <div class="row pb-4" v-for="item in listNews">
                            <div class="col-md-5">
                                <div class="fh5co_hover_news_img">
                                    <div class="fh5co_news_img">
                                        <img v-bind:src="imagesUrl+item.imageName" alt="" />
                                    </div>
                                    <div></div>
                                </div>
                            </div>
                            <div class="col-md-7 animate-box">
                                <a v-bind:href="item.link" class="fh5co_magna py-2">
                                    {{item.head}}
                                </a> <a v-bind:href="item.link" class="fh5co_mini_time py-3">
                                    {{item.date}}
                                </a>
                                <div class="fh5co_consectetur">
                                    {{item.desc}}
                                </div>
                            </div>
                        </div>

                        <div class="row mx-0 animate-box fadeInUp animated-fast" data-animate-effect="fadeInUp">
                            <div class="col-12 text-center pb-4 pt-4">
                                <a href="#" class="btn_mange_pagging"><i class="fa fa-long-arrow-left"></i>&nbsp;&nbsp; {{Previous}}</a>
                                <a href="#" class="btn_pagging ">{{currentPage}}</a>
                                <a href="#" class="btn_mange_pagging">{{Next}} <i class="fa fa-long-arrow-right"></i>&nbsp;&nbsp; </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 animate-box" data-animate-effect="fadeInRight">

                        <div>
                            <div class="fh5co_heading fh5co_heading_border_bottom pt-3 py-2 mb-4">{{mostPopularHeading}}</div>
                        </div>
                        <div class="row pb-3" v-for="item in listMostPopular">
                            <div class="col-5 align-self-center">
                                <img v-bind:src="imagesUrl+item.imageName" alt="img" class="fh5co_most_trading" />
                            </div>
                            <div class="col-7 paddding">
                                <div class="most_fh5co_treding_font"><a v-bind:href="item.link">{{item.desc}}</a>  </div>
                                <div class="most_fh5co_treding_font_123"> {{item.date}}</div>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>


</div>

